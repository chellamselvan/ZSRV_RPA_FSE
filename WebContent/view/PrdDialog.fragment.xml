<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog title="Products" class="sapUiPopupWithPadding"
		items="{/pData/results}" search="_handleValueHelpSearch" confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose">
		<StandardListItem icon="{ProductPicUrl}"
			iconDensityAware="false" iconInset="false" title="{ProductId}"
			description="{ProductDesc}" />
	</SelectDialog>
</core:FragmentDefinition>


